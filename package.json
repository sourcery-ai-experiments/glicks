{
  "name": "glicks",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "updateSchema": "node ./src/scripts/updateSchema.mjs",
    "generateRouter": "node ./src/scripts/generateRouter.cjs",
    "dev": "vite",
    "prebuild": "npm run updateSchema",
    "build": "npm run generateRouter && vite build",
    "build:netlify":"rm -rf ./src/views/admin && rm -rf ./functions && npm run build",
    "preview": "vite preview",
    "validate:schema": "node ./src/scripts/schemaValidation.mjs",
    "addItems": "node ./src/scripts/addItems.mjs",
    "addCategories": "node ./src/scripts/addCategories.mjs",
    "removeCategories": "node ./src/scripts/removeCategories.mjs"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.22",
    "@heroicons/vue": "^2.1.3",
    "ajv-formats": "^3.0.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "netlify-lambda": "^2.0.16",
    "serverless-http": "^3.2.0",
    "sharp": "^0.33.4",
    "uuid": "^10.0.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "vite": "^5.2.0"
  }
}
